{
  "expo": {
    "name": "Featuring-app",
    "slug": "featuring-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/FeatLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/FeatSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/FeatLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vegadev.featuringapp"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/FeatLogo.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://uber.com/"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/FeatLogo.png",
          "color": "#ffffff",
          "sounds": ["./assets/notification-sound.wav"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "503d5abc-4d4d-47ef-81bd-0ccbd546d35a"
      }
    }
  }
}
